
<h1><%= t("visual_code_types") %></h1>

<br />
<table id="visual_code_types" class="index">
  <tr>
    <th>Name</th>
    <th>Handler</th>
  </tr>

<div class="toolbar" data-hook="toolbar">
  <ul class="actions">
  </ul>
  <br class="clear" />
</div>


<% @visual_code_types.each do |vct| %>
  <tr>
    <td><%= vct.name %></td>
    <td><%= vct.handler %></td>
    <td class="actions">
    </td>
  </tr>
<% end %>
</table>
