<p data-hook="visual_code"><%= f.label :visual_codes, t(:singular, :scope => :visual_code) %>: <br />
  <%= f.text_field :visual_code %>
  
  <%= f.field_container :visual_code_types do %>
    <%= f.label :id, t("visual_code.type") %><br />
    <%= f.select :id, Spree::VisualCodeType.all.collect { |vc| [vc.name, vc.id] } %>
  <% end %>
  
  
  <span class="enter_variant_visual_code">
</p>
  

